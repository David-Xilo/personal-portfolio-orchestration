{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "deploy": {
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300,
    "sleepApplication": true,
    "restartPolicyType": "ALWAYS",
    "limitOverride": {
      "containers": {
        "cpu": 0.5,
        "memoryBytes": 536870912
      }
    }
  }
}
