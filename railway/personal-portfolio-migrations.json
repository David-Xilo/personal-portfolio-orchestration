{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "build": {
    "dockerImage": "xilo/personal-portfolio-migrations:latest"
  },
  "deploy": {
    "restartPolicyType": "NEVER",
    "restartPolicyMaxRetries": 0,
    "sleepApplication": false,
    "limitOverride": {
      "containers": {
        "cpu": 0.25,
        "memoryBytes": 268435456
      }
    }
  }
}
